<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="512.0" prefWidth="355.0" style="-fx-background-color: #5e4e4e;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MembersController">
   <children>
   	  <TextField fx:id="name" layoutX="40.0" layoutY="150.0" prefHeight="29.0" prefWidth="221.0" promptText="이름" />
      <TextField fx:id="id" layoutX="41.0" layoutY="192.0" prefHeight="29.0" prefWidth="221.0" promptText="아이디" />
      <PasswordField fx:id="pwd" layoutX="40.0" layoutY="235.0" prefHeight="29.0" prefWidth="221.0" promptText="비밀번호" />
     
      <Label layoutX="117.0" layoutY="63.0" text="회원가입" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="tel" layoutX="41.0" layoutY="275.0" prefHeight="29.0" prefWidth="221.0" promptText="연락처" />
      
      <Button fx:id="submitBtn" layoutX="94.0" layoutY="333.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="73.0" onAction="#submitAction" text="submit" />
      <Button fx:id="cancelBtn" layoutX="178.0" layoutY="333.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="73.0" onAction="#cancelAction" text="cancel" />
   </children>
</AnchorPane>
